      <parameter parameterId="6a0165c7-0fd7-47d6-bfbf-e3fee9aacc42" amount="0.25" variableName="efficiency" unitId="577e242a-461f-44a7-922c-d8e1c3d2bf45">
        <name xml:lang="en">efficiency</name>
