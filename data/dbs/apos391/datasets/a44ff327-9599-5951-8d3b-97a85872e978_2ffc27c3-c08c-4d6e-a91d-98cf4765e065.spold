        <property propertyId="db0b4bbf-fd26-4ba2-b9de-0fd47476304f" amount="0.1237669756874708" isDefiningValue="false" isCalculatedAmount="false" unitId="577e242a-461f-44a7-922c-d8e1c3d2bf45">
