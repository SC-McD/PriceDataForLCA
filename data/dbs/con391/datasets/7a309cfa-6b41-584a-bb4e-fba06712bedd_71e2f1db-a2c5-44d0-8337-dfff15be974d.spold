      <intermediateExchange id="da1649ec-6f29-5405-98f2-1c6c05d39b4c" unitId="487df68b-4994-4027-8fdc-a4dc298257b7" variableName="hard_coal_ash" amount="-0.003625" mathematicalRelation="0.0029 * scaling_factor" sourceId="de441e46-c192-4f88-9876-bf6634be35da" sourceYear="1991" sourceFirstAuthor="Vernon J.L." intermediateExchangeId="76ab0018-e176-43e0-b42c-12e82b937fef" activityLinkId="c1c85115-91f0-58ad-865a-2e8aa258073b">
        <name xml:lang="en">hard coal ash</name>
