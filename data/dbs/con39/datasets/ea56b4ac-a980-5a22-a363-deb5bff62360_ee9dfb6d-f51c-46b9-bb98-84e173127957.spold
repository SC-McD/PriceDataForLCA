  <childActivityDataset>
    <activityDescription>
      <activity specialActivityType="0" id="ea56b4ac-a980-5a22-a363-deb5bff62360" activityNameId="56d999cc-1c26-4d3e-92bb-cd9cc153ae8b" parentActivityId="3b70f023-28ea-45d4-bef0-f35e29391e40" inheritanceDepth="-1" type="1" energyValues="0">
        <activityName xml:lang="en">maintenance, lorry 16 metric ton</activityName>
        <includedActivitiesStart xml:lang="en">The activity starts with the consumption of the components and materials for the maintenance.</includedActivitiesStart>
        <includedActivitiesEnd xml:lang="en">The dataset ends with the maintenance of one lorry 16t. The dataset includes the consumption of the components and materials for the maintenance, the consumption of lead (battery) and until and including the energy consumption (electricity and light fuel oil).</includedActivitiesEnd>
        <generalComment>
          <text xml:lang="en" index="0">The dataset represents the maintenance of one lorry 16t.</text>
        </generalComment>
      </activity>
      <classification classificationId="c7c61217-b562-41bf-b976-28367154f536">
        <classificationSystem xml:lang="en">EcoSpold01Categories</classificationSystem>
        <classificationValue xml:lang="en">transport systems/road</classificationValue>
      </classification>
      <classification classificationId="4627e802-3482-4895-ae5c-cab9840d551b">
        <classificationSystem xml:lang="en">ISIC rev.4 ecoinvent</classificationSystem>
        <classificationValue xml:lang="en">4520:Maintenance and repair of motor vehicles</classificationValue>
      </classification>
      <geography geographyId="f9221622-cc45-11de-98ed-0019e336be3a">
        <shortname xml:lang="en">Europe without Switzerland</shortname>
      </geography>
      <technology technologyLevel="3">
        <comment>
